# Generated by Chef
# Local modifications will be overwritten.
[<%= @repo_name %>]
name            = <%= @description %>
<% if @type %>
type            = <%= @type %>
<% end %>
<% if @url %>
baseurl         = <%= @url %>
<% end %>
<% if @mirrorlist %>
mirrorlist      = <%= @mirrorlist %>
<% end %>
enabled         = <%= @enabled %>
<% if @key %>
gpgcheck        = 1
gpgkey          = file:///etc/pki/rpm-gpg/<%= @key %>
<% else %>
gpgcheck        = 0
<% end %>
<% if @failovermethod %>
failovermethod  = <%= @failovermethod %>
<% end %>
<% if @bootstrapurl %>
bootstrapurl    = <%= @bootstrapurl %>
<% end %>
<% if @includepkgs %>
includepkgs     = <%= @includepkgs %>
<% end %>
